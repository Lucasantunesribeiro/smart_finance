# SmartFinance Terraform Variables
# Copy this file to terraform.tfvars and update the values

# AWS Configuration
aws_region = "us-east-1"

# Project Configuration
project_name = "smartfinance"
environment  = "production"

# SSH Key Configuration
# Generate with: ssh-keygen -t rsa -b 4096 -f ~/.ssh/smartfinance-key
# Then copy the content of ~/.ssh/smartfinance-key.pub here
ssh_public_key = "ssh-rsa  smartfinance-deployment"